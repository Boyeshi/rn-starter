{
  "name": "rn-starter",
  "version": "1.1.1",
  "description": "Production-ready starter for your next React Native App! Powered by cli-rn, React Navigation (v6), RN UI lib, Mobx, Reanimated 2, Dark Mode, Localization, Notifications, Permissions, and much more.",
  "author": "Batyr <dev@batyr.io>",
  "homepage": "https://github.com/kanzitelli/rn-starter",
  "private": false,
  "scripts": {
    "android": "yarn android:adb && npx react-native run-android",
    "android:release": "yarn android --variant release",
    "android:adb": "adb reverse tcp:8081 tcp:8081",
    "ios": "npx react-native run-ios",
    "ios:release": "yarn ios --configuration Release",
    "ios:pods": "npx pod-install",
    "ios:pods:ru": "cd ios && pod install --repo-update && cd ..",
    "start": "npx react-native start",
    "rename": "npx react-native-rename",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@kanzitelli/if-component": "^0.2.1",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/blur": "^3.6.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "date-fns": "^2.27.0",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.8",
    "mobx-persist-store": "^1.0.4",
    "mobx-react": "^7.2.1",
    "react": "17.0.2",
    "react-native": "^0.66.4",
    "react-native-device-info": "^8.4.8",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-localize": "^2.1.6",
    "react-native-mmkv": "^1.5.4",
    "react-native-reanimated": "^2.3.0",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-ui-lib": "^6.5.4",
    "react-native-vector-icons": "^9.0.0",
    "rn-bounceable": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@tsconfig/react-native": "^1.0.4",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "@types/react-native": "^0.66.9",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "metro-react-native-babel-preset": "^0.66.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "17.0.2",
    "release-it": "^14.11.8",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "react",
    "react-native",
    "react-navigation",
    "react-native-starter"
  ],
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT"
}
